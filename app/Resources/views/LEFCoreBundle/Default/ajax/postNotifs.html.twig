{# app\\Resources\\LEF\\CoreBundle\\views\\Default\\ajax\\postNotifs.html.twig #}

{% for notif in postNotifs %}
	{{ include('LEFPostBundle\\templates\\post.html.twig', {'post': notif.post}) }}
{% endfor %}

<div class="sr-only lef-scrollable" id="index-{{ index }}" data-scrollable="{{ scrollable }}" data-url="{{ path('lef_core_post_notifs', {'index': (index + 1)}) }}{{ unseen is defined ? '?unseen=only' }}" data-total="{{ total }}"></div>